{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "noEmit": true,
     "allowJs": true,
 
    "typeRoots": ["./types", "./node_modules/@types"]
 
  }
}
